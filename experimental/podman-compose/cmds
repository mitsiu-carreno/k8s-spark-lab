podman build -t cluster-apache-spark:3.5.3 .

podman-compose up

podman exec -it blog_old_spark-master_1 ./bin/spark-submit --master spark://spark-master:7077 --conf spark.hadoop.fs.s3a.endpoint=http://minio:9000 --conf spark.hadoop.fs.s3a.access.key=accesskey --conf spark.hadoop.fs.s3a.secret.key=secretkey --conf spark.hadoop.fs.s3a.path.style.access=true --packages org.apache.hadoop:hadoop-aws:3.2.0 /opt/spark-apps/test.py
